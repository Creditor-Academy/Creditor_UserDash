# Database
DATABASE_URL=postgresql://username:password@localhost:5432/creditor_db

# JWT Secret
JWT_SECRET=your-super-secret-jwt-key-here

# AI Service Configuration (Generic - hide provider details)
AI_API_KEY=your-ai-service-key-here
AI_PROVIDER_NAME=AI_SERVICE
AI_BASE_URL=https://api.ai-service.com
AI_API_VERSION=v2

# Bytez AI Configuration - Multiple API Keys for Load Balancing
# Primary API key (required)
VITE_BYTEZ_KEY=your-primary-bytez-api-key-here

# Additional API keys for rotation and load balancing (optional but recommended)
VITE_BYTEZ_KEY_2=your-secondary-bytez-api-key-here
VITE_BYTEZ_KEY_3=your-third-bytez-api-key-here
VITE_BYTEZ_KEY_4=your-fourth-bytez-api-key-here

# AI Model Configuration (Optional - uses defaults if not set)
AI_SUMMARY_MODEL=default-summary-model
AI_QA_MODEL=default-qa-model
AI_IMAGE_MODEL=default-image-model
AI_TEXT_MODEL=default-text-model

# AI Service Limits
AI_MAX_RETRIES=3
AI_RETRY_DELAY=1000
AI_RATE_LIMIT=100
AI_DEFAULT_TEMPERATURE=0.3
AI_DEFAULT_MAX_TOKENS=200
AI_REQUEST_TIMEOUT=30000

# Server Configuration
PORT=5000
NODE_ENV=development

# Frontend URL (for CORS)
FRONTEND_URL=http://localhost:3000
